{
  "name": "arase",
  "version": "1.3.0",
  "description": "Official SDK for ARASE - AI-powered search engine API with stocks and weather",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "arase",
    "search",
    "ai",
    "api",
    "saudi",
    "arabic",
    "web-search",
    "llm"
  ],
  "author": "MASARAT SA",
  "license": "MIT",
  "homepage": "https://arase.masarat.sa",
  "repository": {
    "type": "git",
    "url": "https://github.com/masarat-sa/arase-sdk"
  },
  "bugs": {
    "url": "https://github.com/masarat-sa/arase-sdk/issues"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
